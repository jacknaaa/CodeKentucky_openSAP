    "def is_even(number):\n",
    "    return number % 2 == 0\n",
    "\n",
    "for x in range(100):\n",
    "    if is_even(x):\n",
    "        print(f\"{x} is even\")\n",
    "    else:\n",
    "        print(f\"{x} is not even\")\n"
